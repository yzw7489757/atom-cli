{
  "name": "atom-cli",
  "version": "0.0.2",
  "description": "拉取配置模板cli工具",
  "main": "index.js",
  "scripts": {
    "release": "standard-version --release-as",
    "publish:major": "npm run release -- major && npm publish",
    "publish:minor": "npm run release -- minor && npm publish",
    "publish:patch": "npm run release -- patch && npm publish",
    "lint": "eslint --quiet ./src --no-error-on-unmatched-pattern",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "hooks": {
    "pre-commit": "npm run lint"
  },
  "bin": {
    "atom-cli": "./bin/www"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yzw7489757/atom-cli.git"
  },
  "author": "yzw7489757",
  "license": "MIT",
  "keywords": [
    "cli"
  ],
  "files": [
    "lib"
  ],
  "precommit.silent": true,
  "devDependencies": {
    "eslint": "^7.9.0",
    "husky": "^4.3.0",
    "standard-version": "^9.0.0"
  },
  "dependencies": {
    "axios": "^0.20.0",
    "commander": "^6.1.0",
    "consolidate": "^0.16.0",
    "download-git-repo": "^3.0.2",
    "ejs": "^3.1.5",
    "ini": "^1.3.5",
    "inquirer": "^7.3.3",
    "metalsmith": "^2.3.0",
    "ncp": "^2.0.0",
    "ora": "^5.1.0"
  }
}
